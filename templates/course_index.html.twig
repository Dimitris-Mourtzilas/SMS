{% extends 'base.html.twig' %}

{% block title %}Hello CourseController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

    {% if 'courses' is defined %}
<div class="courses-div">
    {% for course in courses %}
        <span class="title-course">Title</span> {{course.getTitle}}
        <span class="ects-course">Title</span> {{course.getTitle}}
        <span class="semester-course">Title</span> {{course.getTitle}}
        <span class="teached-by-course">Title</span> {{course.getTitle}}

    {% endfor %}
</div>
{% endblock %}
